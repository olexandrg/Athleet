<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.azurewebsites.athleet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Athleet">
        <activity android:name=".InviteTeamUser"></activity>
        <activity android:name=".TeamMemberListView" />
        <activity android:name=".Teams.TeamMemberListViewActivity" />
        <activity android:name=".Dashboard.AddTeamActivity" />
        <activity android:name=".Workouts.WorkoutDetailActivity" />
        <activity android:name=".Exercises.AddExerciseActivity" />
        <activity android:name=".Teams.TeamDetailActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Dashboard.DashboardActivity" />
        <activity android:name=".Dashboard.AddWorkoutActivity" />
    </application>

</manifest>